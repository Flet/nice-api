{
  "name": "nice-api",
  "description": "Put a read-only API in front of a google spreadsheet for easier consumption.",
  "version": "1.0.0",
  "homepage": "https://github.com/Flet/nice-api#readme",
  "private": true,
  "main": "src",
  "keywords": [
    "feathers",
    "api",
    "rest",
    "google",
    "sheet"
  ],
  "author": "Dan Flettre <flettre@gmail.com>",
  "contributors": [],
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "type": "module",
  "scripts": {
    "test": "npm run lint && npm run mocha",
    "lint": "standard",
    "dev": "nodemon src/",
    "start": "node src/",
    "mocha": "mocha test/ --recursive --exit"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "ignore": []
  },
  "dependencies": {
    "@feathersjs/configuration": "^5.0.0-pre.26",
    "@feathersjs/errors": "^5.0.0-pre.26",
    "@feathersjs/express": "^5.0.0-pre.26",
    "@feathersjs/feathers": "^5.0.0-pre.26",
    "@feathersjs/transport-commons": "^5.0.0-pre.26",
    "camelcase": "^7.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "feathers-memory": "^4.1.0",
    "google-spreadsheet": "^3.1.15",
    "helmet": "^5.1.0",
    "serve-favicon": "^2.5.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "axios": "^0.27.2",
    "mocha": "^10.0.0",
    "standard": "^17.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Flet/nice-api.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Flet/nice-api/issues"
  }
}
